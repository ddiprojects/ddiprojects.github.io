                var load_w = 0;
                var columns = 3;
                if(self.hasClass('megamenu-columns-2')){
                    columns = 2;
                } else if(self.hasClass('megamenu-columns-3')){
                    columns = 3;
                } else if(self.hasClass('megamenu-columns-4')){
                    columns = 4;
                } else if(self.hasClass('megamenu-columns-5')){
                    columns = 5;
                }

